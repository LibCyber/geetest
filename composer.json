{
  "name": "libcyber/geetest",
  "description": "Geetest for Laravel",
  "keywords": [
    "Geetest",
    "Laravel",
    "LibCyber"
  ],
  "license": "MIT",
  "support": {
    "email": "libcyberstudio@gmail.com"
  },
  "authors": [
    {
      "name": "LibCyber",
      "email": "libcyberstudio@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "illuminate/support": "^6|^7|^8",
    "illuminate/routing": "^6|^7|^8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "LibCyber\\Geetest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LibCyber\\Geetest\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.1-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
